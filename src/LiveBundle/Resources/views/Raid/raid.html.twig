{% extends 'default/live.html.twig' %}

{% block body %}
    <div id="app">
        <social-media-share url="{{ meta['url'] }}"></social-media-share>
    </div>

    {% for tweet in tweets %}
        <ol>
            <li>{{ tweet.text }}</li>
        </ol>
    {% endfor %}
{% endblock %}


{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
{% endblock %}
